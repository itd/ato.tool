<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="ato.tool">
<body>

<tal:comment tal:condition="nothing">

</tal:comment>

<metal:main fill-slot="content-core">
  <metal:content-core define-macro="content-core">

    <h2>
        Rendered from
        compliancefamily_templates/<span tal:replace="template/id" />
    </h2>


    <h2 class=".documentFirstTitle"
         tal:content="context/Title">[[Title inserted here]]</h2>

    <div class="descriptionWrapper">
      <h3>Description</h3>
      <div tal:content="context/description">[[description]]</div>
    </div>

    <div class="descriptionWrapper">
      <h3>Details</h3>
      <div class="docDetails"
        tal:content="context/details">[[details]]</div>
    </div>

  </metal:content-core>
</metal:main>

</body>
</html>

